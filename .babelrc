{
  "presets": [
    "es2015",
    "react"
  ],
  "env": {
    "development": {
    // https://github.com/gaearon/react-transform-hmr
    "plugins": [["react-transform", {
       "transforms": [{
         "transform": "react-transform-hmr",
         "imports": ["react"],
         "locals": ["module"]
       }]
     }]]
    }
  }
}
